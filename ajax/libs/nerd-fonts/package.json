{
  "name": "nerd-fonts",
  "description": "Iconic font aggregator, collection, and patcher: 39 patched fonts (87k+ variations). Includes popular glyph collections such as Font Awesome & fonts such as Hack",
  "keywords": [
    "fonts",
    "patched-fonts",
    "powerline",
    "shell",
    "statusline",
    "python",
    "iconic-fonts",
    "patcher",
    "font-awesome",
    "octicons"
  ],
  "author": "Ryan L McIntyre",
  "license": "MIT",
  "homepage": "https://github.com/ryanoasis/nerd-fonts#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/ryanoasis/nerd-fonts.git"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/ryanoasis/nerd-fonts.git",
    "fileMap": [
      {
        "basePath": "patched-fonts",
        "files": [
          "**/*"
        ]
      }
    ]
